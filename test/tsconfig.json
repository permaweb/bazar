{
	"compilerOptions": {
		"target": "es2018",
		"module": "commonjs",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"strict": false,
		"baseUrl": "..",
		"paths": {
			"*": ["*", "bazar-studio/*"]
		}
	},
	"include": ["../**/*.ts"],
	"exclude": ["node_modules"]
}
